{
  "name": "hono-telescope",
  "version": "0.1.4",
  "type": "module",
  "author": {
    "name": "İlker Balcılar",
    "email": "ilkerbalcilartr@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jubstaaa/hono-telescope.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@release-it/conventional-changelog": "^8.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.0.0",
    "release-it": "^17.0.0",
    "turbo": "latest",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.46.2"
  },
  "peerDependencies": {
    "hono": ">=3.0.0"
  },
  "bugs": {
    "url": "https://github.com/jubstaaa/hono-telescope/issues"
  },
  "description": "A powerful debugging and monitoring tool for Hono applications, inspired by Laravel Telescope",
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "homepage": "https://github.com/jubstaaa/hono-telescope#readme",
  "keywords": [
    "hono",
    "debugging",
    "monitoring",
    "telescope",
    "middleware",
    "observability",
    "performance",
    "typescript",
    "bun",
    "nodejs"
  ],
  "license": "MIT",
  "packageManager": "bun@1.3.1",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "turbo run start",
    "build": "turbo run build && mkdir -p dist && cp -r apps/core/dist/* dist/",
    "dev": "turbo run dev",
    "dev:example": "turbo run dev --filter=@hono-telescope/example",
    "dev:dashboard": "turbo run dev --filter=@hono-telescope/dashboard",
    "test": "turbo run test",
    "clean": "turbo clean && rm -rf node_modules dist",
    "prepublishOnly": "npm run build",
    "type-check": "turbo run type-check",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "release": "release-it"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
